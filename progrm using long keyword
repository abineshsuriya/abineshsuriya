#include<stdio.h>
int main()
{
int a;
long int b;
long long int c;
double d;
long double e;
printf("Size of int = %zu bytes\n",size of (a));
printf("Size of long int = %zu bytes\n",size of(b));
printf("Size of long long int = %zu bytes\n",size of(c));
printf("Size of double = %zu bytes\n",size of(d));
printf("Size of long double = %zu bytes\n",size of(e));
return 0;
}
